# .env - Environment variables for the data sync project

# Xiaoe Platform Credentials
XIAOE_APP_ID=
XIAOE_CLIENT_ID=
XIAOE_SECRET_KEY= 

# MySQL Database Credentials
MYSQL_USER=
MYSQL_PASSWORD=
MYSQL_HOST=127.0..1
MYSQL_PORT=
MYSQL_DB_NAME=

# PostgreSQL Database Credentials (Optional - uncomment and fill if used)
# DB_USER=your_pg_user
# DB_PASSWORD=your_pg_password
# DB_HOST=localhost
# DB_PORT=5432
# DB_NAME=your_pg_db_name

# Logging Level (DEBUG, INFO, WARNING, ERROR, CRITICAL)
LOG_LEVEL=INFO

SYNC_ORDERS_DAYS_BACK=1
SYNC_AFTERSALES_DAYS_BACK=1